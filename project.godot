config_version=5

[application]

config/name="Keywer Defence 2D"
config/tags=PackedStringArray("2d")
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.4", "Mobile")

[autoload]

UiInteractionManager="*res://Managers/ui_interaction_manager.gd"
GameManager="*res://Managers/game_manager.gd"
WeaponDatabase="*res://Managers/weapon_database.gd"
StateMachine="*res://Managers/state_machine.gd"
WorkshopManager="*res://Managers/workshop_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Managers/": "green",
"res://Scenes/": "green",
"res://Scenes/Weapons/": "orange",
"res://Scripts/": "blue"
}

